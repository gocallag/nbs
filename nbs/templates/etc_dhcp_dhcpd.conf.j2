# ISC DHCPD configuration -- don't edit manually!
#
# {{ ansible_managed }}

allow bootp;
allow booting;
max-lease-time 1200;
default-lease-time 900;
log-facility local7;

option ip-forwarding    false;
option mask-supplier    false;

   subnet 10.0.255.0 netmask 255.255.255.0 {

       option  routers   10.0.255.254;
       option  domain-name-servers  10.0.255.254;
       range 10.0.255.100 10.0.255.250;
       next-server 10.0.255.254;
       filename "pxelinux.0";
   }
